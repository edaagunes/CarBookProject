@model UpdatePricingDto
@{
    ViewData["Title"] = "UpdatePricing";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}
<div class="page-content">
	<div class="container-fluid">
		<div class="row">
			<div class="col-xl-12">
				<div class="card">
					<div class="card-body">

						<h4 class="card-title">Ödeme Periyodu Güncelleme</h4>
						<p class="card-subtitle mb-4"></p>

						<form method="post">
							<input type="hidden" asp-for="PricingId"/>
							<div class="form-group">
								<label for="exampleInputModel">Ödeme Periyodu Bilgisi</label>
								<input type="text" class="form-control" id="exampleInputModel" asp-for="Name" />
							</div>
							<button type="submit" class="btn btn-primary waves-effect waves-light">Ödeme Periyodunu Güncelle</button>
						</form>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

